{{#each listProd}}
<body>
<div class="wrapper">
        <div class="user-card">
            <div class="user-card-img">
              <img src="{{thumbnail}}" alt="">
            </div>
            <div class="user-card-info">
              <h2> {{title}} </h2>
              <p><span> {{description}} </span></p>
              <p><span> {{category}} </span> </p>
              <p><span> {{code}} </span> </p>
              <p><span>Price: uSd {{price}} </span></p>
              <button class="batc">Add to Cart</button>
            </div>
        </div>
    </div>
      
</body>
{{/each}}